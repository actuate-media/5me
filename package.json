{
  "name": "5me",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npm run prisma:generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "npm run prisma:generate",
    "prisma:generate": "prisma generate --schema=prisma/schema-core.prisma && prisma generate --schema=prisma/schema-widgets.prisma",
    "db:push": "prisma db push --schema=prisma/schema-core.prisma && prisma db push --schema=prisma/schema-widgets.prisma",
    "db:migrate": "prisma migrate dev --schema=prisma/schema-core.prisma && prisma migrate dev --schema=prisma/schema-widgets.prisma",
    "db:migrate:deploy": "prisma migrate deploy --schema=prisma/schema-core.prisma && prisma migrate deploy --schema=prisma/schema-widgets.prisma",
    "db:studio:core": "prisma studio --schema=prisma/schema-core.prisma",
    "db:studio:widgets": "prisma studio --schema=prisma/schema-widgets.prisma",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force && npm run db:seed",
    "db:validate": "prisma validate --schema=prisma/schema-core.prisma && prisma validate --schema=prisma/schema-widgets.prisma"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@tanstack/react-query": "^5.90.12",
    "@vercel/blob": "^2.0.0",
    "@vercel/edge-config": "^1.4.0",
    "@vercel/postgres": "^0.10.0",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.561.0",
    "next": "^16.1.1",
    "next-auth": "^5.0.0-beta.25",
    "prisma": "^5.22.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-image-crop": "^11.0.10",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.1.1",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3"
  }
}
